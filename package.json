{
  "name": "morerecipes",
  "version": "1.0.0",
  "description": "More Recipes provides a platform for users to share awesome and exciting recipe ideas they have invented or learnt.",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/bin/www.js",
    "dev": "nodemon server/bin/www --exec babel-node",
    "babel-node": "babel-node --",
    "build": "babel ./server -d ./build",
    "heroku-postbuild": "npm run build && npm run migrate",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "unmigrate": "node_modules/.bin/sequelize db:migrate:undo:all",
    "dist": "./node_modules/.bin/babel server -d dist --presets=babili",
    "test": "NODE_ENV=test mocha server/__test__/*.js --exit --require babel-register --reporter spec",
    "coverage": "nyc reports --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sccofield/moreRecipes.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sccofield/moreRecipes/issues"
  },
  "nyc": {
    "include": [
      "server/**/*.js"
    ]
  },
  "homepage": "https://github.com/sccofield/moreRecipes#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili": "^0.1.4",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "express": "^4.16.2",
    "express-validator": "^4.3.0",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.20.1",
    "sequelize-cli": "^3.2.0"
  },
  "devDependencies": {},
  "engines": {
    "node": "8.4.0",
    "npm": "5.4.2"
  }
}
