{
  "name": "morerecipes",
  "version": "1.0.0",
  "description": "More Recipes provides a platform for users to share awesome and exciting recipe ideas they have invented or learnt.",
  "main": "index.js",
  "scripts": {
    "start": "node ./build/bin/www.js",
    "dev": "nodemon server/bin/www --exec babel-node",
    "e2e:server": "NODE_ENV=test babel-node server/bin/www",
    "client:dev": "babel-node server/bin/www",
    "babel-node": "babel-node --",
    "build-client": "webpack --config ./webpack.prod.js --progress",
    "build": "babel ./server -d ./build && npm run build-client",
    "heroku-postbuild": "npm run build",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "unmigrate": "node_modules/.bin/sequelize db:migrate:undo:all",
    "reset-migrate": "npm run unmigrate && npm run migrate",
    "dist": "./node_modules/.bin/babel server -d dist --presets=babili",
    "test": "NODE_ENV=test npm run reset-migrate && NODE_ENV=test mocha server/__test__/*.js --require babel-register --reporter spec --exit",
    "test:client": "jest && npm run client:coverage",
    "coverage": "nyc reports --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "test-client": "jest client/__test__/**/** --watch --coverage",
    "e2e:test": "NODE_ENV=test npm run reset-migrate && nightwatch ./e2eTest/tests.spec.js",
    "e2e:setup": "selenium-standalone install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sccofield/moreRecipes.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sccofield/moreRecipes/issues"
  },
  "nyc": {
    "include": [
      "server/**/*.js"
    ]
  },
  "homepage": "https://github.com/sccofield/moreRecipes#readme",
  "dependencies": {
    "@tinymce/tinymce-react": "^2.2.0",
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili": "^0.1.4",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "expect": "^22.4.0",
    "express": "^4.16.2",
    "express-validator": "^4.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "history": "^4.7.2",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "moxios": "^0.4.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.6.0",
    "querystring": "^0.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-dropzone": "^4.2.7",
    "react-paginate": "^5.1.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "selenium-standalone": "^6.12.0",
    "sequelize": "^4.20.1",
    "sequelize-cli": "^3.2.0",
    "toastr": "^2.1.4",
    "validator": "^9.2.0",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.9",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^1.1.6",
    "jest": "^22.4.2",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.2"
  },
  "engines": {
    "node": "8.4.0",
    "npm": "5.4.2"
  }
}
